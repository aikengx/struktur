<h2>Product Management</h2>
<div>
  {{#each hbsProducts}}
  <ul>
    <li><strong>Name: </strong>{{this.name}}</li>
    <li><strong>Description: </strong>{{this.description}}</li>
    <li><strong>System: </strong><em>{{this.system}}</em></li>
    <li><strong>URL: </strong><em>{{this.url}}</em></li> 
    <li><strong>Image: </strong><em>{{this.imgUrl}}</em></li> 
    <li><strong>Part Number: </strong><em>{{this.part_number}}</em></li> 
    <li><strong>Description: </strong><em>{{this.description}}</em></li> 
    <li><strong>Created by: </strong>{{this.created_by}}</li>
  </ul>
  <div>
    <a href="/product/update/{{this._id}}">Edit</a> |
    <a href="/product/delete/{{this._id}}">Delete</a>
  </div>
  <hr>
  {{/each}}
</div>